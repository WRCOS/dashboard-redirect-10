<!DOCTYPE html>
<html>
<head>
  <title>Dynamic ArcGIS Redirect</title>
  <script>
    window.onload = function() {
      // Calculate today minus 10 days
      var today = new Date();
      today.setDate(today.getDate() - 10);
      // Format as YYYY-MM-DD
      var yyyy = today.getFullYear();
      var mm = String(today.getMonth() + 1).padStart(2, '0');
      var dd = String(today.getDate()).padStart(2, '0');
      var dateString = yyyy + '-' + mm + '-' + dd;
      // Construct your dashboard URL (replace CreatedDate with your actual parameter)
      var dashboardBase = 'https://srqgis.sarasotafl.gov/portal/apps/dashboards/9155dbab0dca440190b5d0e25eca6b9b';
      var redirectURL = dashboardBase + '#CreatedDate=' + dateString + ',';
      window.location.replace(redirectURL);
    };
  </script>
</head>
<body>
  Redirecting to the latest assignments...
</body>
</html>
